
threshold: medium
fileignoreconfig:
- filename: .github/workflows/CI.yml
  checksum: 7d8b743bc4d62611a102ac69b25e43decf6e40e66eb8c234f0f48cff1d9858d0
- filename: '*.yml'
  ignore_detectors: [filename] 

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium


allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}   