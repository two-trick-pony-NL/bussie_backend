
threshold: medium
fileignoreconfig:
- filename: .github/workflows/CI.yml
  checksum: 08ecdd4d4ec2a1676b32e4c36344b77050027ace9adde7906ccec1d21a1667cb
- filename: '*.yml'
  ignore_detectors: [filename] 

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium


allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}   