fileignoreconfig:
- filename: .github/workflows/CI.yml
  checksum: 143995676a28a308afb4c3940743835096f3b888789cbdd439bbc365bfe9f3b5
- filename: '*.yml'
  ignore_detectors: [filename] 

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium


allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}   