
threshold: medium
fileignoreconfig:
- filename: .github/workflows/CI.yml
  checksum: 1cd18d847679c27ac495b95c1b28714867fd94a1fc4e6cbf636c75e062d08294
- filename: '*.yml'
  ignore_detectors: [filename] 

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium


allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}   